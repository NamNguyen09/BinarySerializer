<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>1.0.0</Version>
    <Authors>Tom Lee</Authors>
    <Company>Conexus AS</Company>
    <Copyright>Copyright (c) 2023 Tom Lee</Copyright>
    <PackageProjectUrl>https://github.com/NamNguyen09/BinarySerializer</PackageProjectUrl>
    <RepositoryUrl>https://github.com/NamNguyen09/BinarySerializer</RepositoryUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <None Update="LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
